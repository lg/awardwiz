{
  "name": "awardwiz",
  "private": true,
  "version": "2.0.0",
  "packageManager": "pnpm@7.14.0",
  "scripts": {
    "test": "NODE_NO_WARNINGS=1 vitest",
    "test-scrapers": "NODE_NO_WARNINGS=1 VITE_LIVE_SCRAPER_TESTS=1 vitest scrapers",
    "coverage": "NODE_NO_WARNINGS=1 vitest run --coverage",
    "build": "pnpm run schemas && tsc && vite build",
    "preview": "vite preview",
    "start": "pnpm run schemas && concurrently --kill-others --names firebase,vite,tsc-watch --prefix-colors yellow,white,cyan \"pnpm start-emulators\" \"vite\" \"cd functions; pnpm tsc -w --preserveWatchOutput\" ",
    "start-emulators": "pnpm exec firebase emulators:start",
    "depcheck": "depcheck --ignores @iconify/json,~icons,@vitest/coverage-c8,vite-node,node-fetch",
    "schemas": "json2ts -i 'src/**/*.schema.json' -o 'src/types/'",
    "prepare": "husky install",
    "check": "NODE_NO_WARNINGS=1 pnpm run depcheck && echo 'linting' && TIMING=1 eslint --cache . && pnpm test run && SHOW_VIZ=false pnpm build --log-level=error && cd functions && pnpm check && echo 'ok'",
    "lets-upgrade-packages": "NPM_CHECK_INSTALLER=pnpm npm-check -u",
    "marked-fares-worker": "NODE_OPTIONS=--no-experimental-fetch NODE_NO_WARNINGS=1 vite-node src/workers/marked-fares.ts",
    "preview-email-worker": "vite-node src/workers/preview-email.ts",
    "deploy-functions": "pnpm firebase deploy --only functions --force --debug"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@ant-design/icons": "4.8.0",
    "@react-oauth/google": "0.5.0",
    "@tanstack/query-sync-storage-persister": "4.15.1",
    "@tanstack/react-query": "4.16.1",
    "@tanstack/react-query-persist-client": "4.16.1",
    "antd": "5.0.1",
    "axios": "1.2.0",
    "dayjs": "^1.11.6",
    "firebase": "^9.14.0",
    "firebase-admin": "^11.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "7.7.0",
    "@iconify/json": "2.1.143",
    "@svgr/core": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/nodemailer": "^6.4.6",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "0.25.3",
    "concurrently": "^7.6.0",
    "depcheck": "^1.4.3",
    "eslint": "8.28.0",
    "eslint-plugin-deprecation": "1.3.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "firebase-tools": "^11.17.0",
    "handlebars": "^4.7.7",
    "husky": "^8.0.2",
    "jsdom": "20.0.3",
    "json-schema-to-typescript": "^11.0.2",
    "lint-staged": "13.0.4",
    "listr2": "^5.0.5",
    "nodemailer": "^6.8.0",
    "npm-check": "^6.0.1",
    "p-wait-for": "^5.0.0",
    "puppeteer": "19.3.0",
    "rollup-plugin-visualizer": "^5.8.3",
    "unplugin-icons": "0.14.14",
    "vite": "3.2.4",
    "vite-node": "0.25.3",
    "vite-plugin-watch-and-run": "1.1.0",
    "vitest": "0.25.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@tanstack/query-core",
        "@firebase/app-types"
      ]
    }
  },
  "eslintIgnore": [
    "src/types/*.schema.d.ts",
    "dist/",
    "functions/lib/",
    "tools/",
    "vite-plugin-firebase.ts"
  ]
}
